<section>

    <div class="container" style="margin-top: 5em;">
        <%if(address){%>
            <% if( address.length> 0){ %>
              <% for(let i=1;i <=address.length; i++){ %>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
              <div class="input-group-text">
                <input type="checkbox" aria-label="Checkbox for following text input">
              </div>
            </div>
            <input type="text" class="form-control" aria-label="<%=address[i-1].Name%>">
            <input type="text" class="form-control" aria-label="<%=address[i-1].Mobile%>">
            <input type="text" class="form-control" aria-label="<%=address[i-1].Address%>">
          </div>
          <% }} else { %>
            <tr>
                <td colspan="5">no address exists</td>
            </tr>
            <% } %>
            <%} %>
    </div>
  </section>